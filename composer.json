{
	"name" : "dafap/SchoolBusManager",
	"description" : "School Bus Manager, software - Logiciel de gestion des transports scolaires full-web",
	"version" : "2.4.0",
	"support" : {
		"email" : "sbm@dafap.fr",
		"issues" : "https://github.com/dafap/SchoolBusManager/issues",
		"wiki" : "https://github.com/dafap/SchoolBusManager/wiki",
		"source" : "https://github.com/dafap/SchoolBusManager"
	},
	"keywords" : [
		"transport",
		"school",
		"framework",
		"zf2",
		"transport scolaire"
	],
	"homepage" : "https://www.dafap.fr/",
	"license" : "CECILL-B",
	"time" : "2014-09-01",
	"require" : {
		"atukai/at-php-settings" : "~1.1",
		"php" : "^5.6 || ^7.0",
		"zendframework/zendframework" : "~2.5",
		"tecnickcom/tcpdf" : "~6.2",
		"drewm/mailchimp-api" : "~2.5",
		"ocramius/package-versions" : "~1.3"
	},
	"require-dev" : {
		"zendframework/zend-developer-tools" : "~1.2"
	},
	"minimum-stability" : "stable",
	"repositories" : [{
			"type" : "composer",
			"url" : "https://packages.zendframework.com/"
		}
	],
	"prefer-stable" : true,
	"config" : {
		"process-timeout" : 5000,
		"use-include-path" : false,
		"github-protocols" : [
			"git",
			"https",
			"http"
		],
		"vendor-dir" : "vendor"
	},
	"autoload" : {
		"classmap" : [
			"vendor/tecnickcom/tcpdf"
		],
		"psr-4" : {
			"SbmBase\\" : "module/SbmBase/src",
			"SbmAdmin\\" : "module/SbmAdmin/src",
			"SbmAjax\\" : "module/SbmAjax/src",
			"SbmAuthentification\\" : "module/SbmAuthentification/src",
			"SbmCartographie\\" : "module/SbmCartographie/src",
			"SbmCommun\\" : "module/SbmCommun/src",
			"SbmFront\\" : "module/SbmFront/src",
			"SbmGestion\\" : "module/SbmGestion/src",
			"SbmInstallation\\" : "module/SbmInstallation/src",
			"SbmMail\\" : "module/SbmMail/src",
			"SbmMailChimp\\" : "module/SbmMailChimp/src",
			"SbmPaiement\\" : "module/SbmPaiement/src",
			"SbmParent\\" : "module/SbmParent/src",
			"SbmPdf\\" : "module/SbmPdf/src",
			"SbmPortail\\" : "module/SbmPortail/src"
		}
	},
	"authors" : [{
			"name" : "Pomirol",
			"email" : "sbm@dafap.fr",
			"homepage" : "https://www.dafap.fr/blog",
			"role" : "Développeur indépendant"
		}
	]
}